local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = 'Key System'})

Key = "test"

local Tab = Window:MakeTab({
    Name = "Key System",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        if Value == Key then
            OrionLib:MakeNotification({
                Name = "Success",
                Content = "Key Is Correct",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            OrionLib:Destroy()
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Key Is Incorrect",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end   
})
